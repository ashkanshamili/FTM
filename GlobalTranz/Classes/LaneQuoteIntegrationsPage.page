<apex:page standardController="FreightTM__Lane_Quote__c" extensions="LaneQuoteIntegrations"  showheader="false" sidebar="false" cache="true" docType="html-5.0" applyBodyTag="false" lightningStylesheets="true" standardStylesheets="false" >
    
    <apex:form >
        <head>
            <meta http-equiv="Content-Type" content="text/html;charset=UTF-8" />
            <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
            <meta name="viewport" content="width=device-width, initial-scale=1.0" />
            <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" />
            <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css"/>
            <style>
                @import url('https://fonts.googleapis.com/css?family=Roboto');
                * {
                margin: 0;
                padding: 0;
                box-sizing: border-box;
                }
                body{
                text-align: center;
                
                }
                .wraper{
                max-width: fullscreen;
                }
                .container{
                margin: 0;
                max-width: 1200px;
                }
                .button{
                margin: 0 auto;
                dysplay: inline;
                }
                
                .wraperBody{
                
                background-color: #b7b8b9;
                
                }
                .formWraper{
                background-color: white;
                margin: 20px 15px 15px 15px;
                }
                .secCol{
                color: white;
                background-color: #5b90c5;
                }
                .red-text {
                color: red;
                }
                .customerError { font-weight: bold; list-style: none; }
                .invoiceError { color: red; font-weight: strong; }
                .bkg-inv { background-color: #eee; }
                .products tr { border: solid 1px #efefef; }
                .container { padding: 0% 5%;}
                .text { padding: 1%;}
                
            </style>
            <script>
            function redirectToPreviousPage() {
                window.history.back();
            }
            </script>
            
        </head>
        <body>
            
            
            <div class="wraper-md mt-3 mb-3">     
                <div class="container-md">

                    <!-- ===== Load Details ===== -->
                    <div class="bkg-inv">
                        <div class="text">
                            <table width="100%">
                                <tr style="text-align: left;vertical-align: top;">
                                    <td width="20%" style="text-align: left;vertical-align: top;">
                                        <b>Load No:</b>&nbsp;<apex:outputField value="{!FreightTM__Lane_Quote__c.Name}"/><br />
                                        <b>Customer:</b>&nbsp;<apex:outputField value="{!FreightTM__Lane_Quote__c.FreightTM__Customer__r.Name}"/><br />
                                        <b>Customer Id:</b>&nbsp;<apex:outputField value="{!FreightTM__Lane_Quote__c.Customer_Id__c}"/><br />
                                        <b>Contact:</b>&nbsp;<apex:outputField value="{!FreightTM__Lane_Quote__c.Contact_Name__c}"/><br />
                                        <b>Date:</b>&nbsp;{!MONTH(TODAY())}/{!DAY(TODAY())}/{!YEAR(TODAY())}<br />
                                    </td>
                                    
                                    <td width="20%" style="text-align: left;vertical-align: top;">
                                        <b>Pickup Address:</b><br />
                                        <apex:outputText value="{!FreightTM__Lane_Quote__c.FreightTM__Pickup_Facility__r.Name}" /><br />
                                        <apex:outputField value="{!FreightTM__Lane_Quote__c.FreightTM__Pickup_Street__c}"/><br />
                                        <apex:outputPanel ><apex:outputText value="{!FreightTM__Lane_Quote__c.FreightTM__Pickup_City__c}"/>,&nbsp; 
                                            <apex:outputText value=" {!FreightTM__Lane_Quote__c.FreightTM__Pickup_State__c}"/>&nbsp; 
                                            <apex:outputText value=" {!FreightTM__Lane_Quote__c.FreightTM__Pickup_Zip_Code__c}"/></apex:outputPanel><br />
                                        <apex:outputField value="{!FreightTM__Lane_Quote__c.FreightTM__Pickup_Country__c}"/><br />
                                    </td>
                                    <td width="20%" style="text-align: left;vertical-align: top;">
                                        <b>Delivery Address:</b><br />
                                        <apex:outputText value="{!FreightTM__Lane_Quote__c.FreightTM__Delivery_Facility__r.Name}" /><br />
                                        <apex:outputField value="{!FreightTM__Lane_Quote__c.FreightTM__Delivery_Street__c}"/><br />
                                        <apex:outputPanel ><apex:outputText value="{!FreightTM__Lane_Quote__c.FreightTM__Delivery_City__c}"/>,&nbsp; 
                                            <apex:outputText value=" {!FreightTM__Lane_Quote__c.FreightTM__Delivery_State__c}"/> &nbsp;
                                            <apex:outputText value=" {!FreightTM__Lane_Quote__c.FreightTM__Delivery_Zip_Code__c}"/>
                                        </apex:outputPanel><br />
                                        <apex:outputField value="{!FreightTM__Lane_Quote__c.FreightTM__Delivery_Country__c}"/><br /> 
                                    </td>
                                    <td width="20%" style="text-align: left;vertical-align: top;">
                                        <b>No of Pallets:</b>&nbsp;<apex:outputField value="{!FreightTM__Lane_Quote__c.Number_of_Pallets__c}"/><br /> 
                                        <b>No of Pieces:</b>&nbsp;<apex:outputField value="{!FreightTM__Lane_Quote__c.Number_of_Pieces__c}"/><br /> 
                                        <b>Hazmat:</b>&nbsp;<apex:outputText value="{!IF(FreightTM__Lane_Quote__c.Hazmat__c, 'Yes', 'No')}"/><br />
                                        <b>Hazmat Class:</b>&nbsp;<apex:outputField value="{!FreightTM__Lane_Quote__c.Hazmat_Class__c}"/><br />
                                        <b>Stackable</b>&nbsp;<apex:outputText value="{!IF(FreightTM__Lane_Quote__c.Stackable__c, 'Yes', 'No')}"/><br /> 
                                        
                                    </td>
                                    <td width="25%" style="text-align: left;vertical-align: top;">
                                        <b>Length:</b>&nbsp;<apex:outputField value="{!FreightTM__Lane_Quote__c.Length__c}"/><br /> 
                                        <b>Width:</b>&nbsp;<apex:outputField value="{!FreightTM__Lane_Quote__c.Width__c}"/><br /> 
                                        <b>Height:</b>&nbsp;<apex:outputField value="{!FreightTM__Lane_Quote__c.Height__c}"/><br /> 
                                        <b>Weight:</b>&nbsp;<apex:outputField value="{!FreightTM__Lane_Quote__c.FreightTM__Weight__c}"/><br /> 
                                        <b>Freight Class:</b>&nbsp;<apex:outputField value="{!FreightTM__Lane_Quote__c.Class__c}"/><br />
                                        
                                    </td>
                                </tr>
                            </table>
                        </div></div>
                    <hr/>
                    <!-- Get rate Button-->
                    
                    <apex:commandButton value="Get Rate"
                                        action="{!getData}"
                                        immediate="false"
                                        rerender="gtzAmt,gtzTran,gtzQid,gtzEst,gtzMsg,objectNamePanel"
                                        styleClass="btn btn-primary btn-sm "/>
                    
                    <hr/>
                    <apex:pageMessages id="pageMsgs"/>
                    <div class="wraperBody border">
                        <div class="formWraper border rounded">
                            <div class="row m-5 b-5">
                                
                                <!-- GlobalTranz -->
                                <div class="col-sm-3  border rounded">
                                    
                                    <!--<img src="{!$Resource.GT_Logo}"  width="60" class="card-img-top border rounded mt-2" alt="icon"/>-->
                                    <p class="card-title mt-2"><b>GlobalTranz</b></p>
                                    <p class="card-text"><b>Cheapest Service</b></p>
                                    <!-- Quote Id -->
                                    <p class="card-text">
                                        <b>Quote Id:</b><br/>
                                        <apex:outputText id="gtzQid" value="{!gtzQuoteId}"/>
                                    </p>
                                    <!-- Amount -->
                                    <p class="card-text">
                                        <b>Amount:</b><br/>
                                        $<apex:outputText id="gtzAmt" value="{!gtzAmount}"/>
                                    </p>
                                    <!-- Transit days -->
                                    <p class="card-text">
                                        <b>Transit Days:</b><br/>
                                        <apex:outputText id="gtzTran" value="{!gtzTransit}"/>
                                    </p>
                                    <p class="card-text mb-3">
                                        <b>Est. Delivery:</b><br/>
                                        <apex:outputText id="gtzEst" value="{!gtzEstDelivery}"/>
                                    </p>
                                    <!-- Custom Message -->
                                    <p class="card-text mb-3">
                                        <b>Message:</b><br/>
                                        <apex:outputText id="gtzMsg" value="{!gtzMessage}"/>
                                    </p>
                                    <br />

                                    <apex:commandButton value="Book Shipment"
                                                        action="{!bookForGlobalTranz}"
                                                        rerender="pageMsgs"
                                                        styleClass="btn btn-primary btn-sm btn-block mt-3 mb-3"/>
                                    <br />
                                </div>
                                
                                <!-- BlueGrace -->
                                <div class="col-sm-3 border rounded">
                                    <!--<img src="{!$Resource.BG_Logo}"  width="60" class="card-img-top border rounded mt-2" alt="icon"/>-->
                                    <p class="card-title mt-2"><b>BlueGrace</b></p>
                                    <p class="card-text"><b>Standard Service</b></p>
                                    <!-- Quote Id -->
                                    <p class="card-text">
                                        <b>Quote Id:</b><br/>
                                        
                                    </p>
                                    <!-- Amount -->
                                    <p class="card-text">
                                        <b>Amount:</b><br/>
                                        $
                                    </p>
                                    <!-- Transit days -->
                                    <p class="card-text">
                                        <b>Transit Days:</b><br/>
                                        
                                    </p>
                                    <p class="card-text mb-3">Est. Delivery</p>
                                    <!-- Custom Message -->
                                    <p class="card-text mb-3">
                                        <b>Message:</b><br/>
                                        
                                    </p>
                                    <br />
                                    
                                    
                                    <apex:commandButton value="Book Shipment" action="{!bookFor}" styleClass="btn btn-primary btn-sm btn-block mt-3 mb-3"/>
                                    
                                    <br />
                                </div>
                                
                                <!-- Air Forward -->
                                <div class="col-sm-3 border rounded">
                                    <!--<img src="{!$Resource.Other_Logo}" height="120"  class="card-img-top border rounded mt-2" alt="icon"/>-->
                                    <p class="card-title mt-1"><b>Air Forward</b></p>
                                    <p class="card-text"><b>Terminal Service</b></p>
                                    <!-- Quote Id -->
                                    <p class="card-text">
                                        <b>Quote Id:</b><br/>
                                        
                                    </p>
                                    <!-- Amount -->
                                    <p class="card-text">
                                        <b>Amount:</b><br/>
                                        $
                                    </p>
                                    <!-- Transit days -->
                                    <p class="card-text">
                                        <b>Transit Days:</b><br/>
                                        
                                    </p>
                                    <p class="card-text mb-3">Est. Delivery</p>
                                    <!-- Custom Message -->
                                    <p class="card-text mb-3">
                                        <b>Message:</b><br/>
                                        
                                    </p>
                                    <br />
                                    
                                    
                                    <apex:commandButton value="Book Shipment" action="{!bookFor}" styleClass="btn btn-primary btn-sm btn-block mt-3 mb-3"/>
                                    
                                    <br />
                                </div>
                                
                                <div class="col-sm-3 border rounded">
                                    <!--<img src="{!$Resource.Other_Logo}" height="120"  class="card-img-top border rounded mt-2" alt="icon"/>-->
                                    <p class="card-title mt-1"><b>Air Forward</b></p>
                                    <p class="card-text"><b>Door2Door Service</b></p>
                                    <!-- Quote Id -->
                                    <p class="card-text">
                                        <b>Quote Id:</b><br/>
                                        
                                    </p>
                                    <!-- Amount -->
                                    <p class="card-text">
                                        <b>Amount:</b><br/>
                                        $
                                    </p>
                                    <!-- Transit days -->
                                    <p class="card-text">
                                        <b>Transit Days:</b><br/>
                                        
                                    </p>
                                    <p class="card-text mb-3">Est. Delivery</p>
                                    <!-- Custom Message -->
                                    <p class="card-text mb-3">
                                        <b>Message:</b><br/>
                                        
                                    </p>
                                    <br />
                                    
                                    
                                    <apex:commandButton value="Book Shipment" action="{!bookFor}" styleClass="btn btn-primary btn-sm btn-block mt-3 mb-3"/>
                                    
                                    <br />
                                </div>
                                
                                
                            </div>
                            
                            
                            
                            
                            
                        </div>
                        
                    </div>
                </div> 
            </div>
            
        </body>
    </apex:form>
</apex:page>